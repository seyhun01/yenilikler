
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Majestic Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Admin/vendors/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <link rel="stylesheet" href="~/Admin/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Admin/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Admin/images/favicon.png" />
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
      
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <a asp-controller="Size" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-grid-large menu-icon"></i>
                            <span class="menu-title">Ölçülər</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Category" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Kateqoriya</span>
                        </a>
                    </li>
                   

                    <li class="nav-item">
                        <a asp-controller="Product" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title"> Məhsullar</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="SaleOffProduct" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Endirimli Məhsullar</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="FavoriteProduct" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-emoticon menu-icon"></i>
                            <span class="menu-title">Sevimli Məhsullar</span>
                        </a>
                  
                    <li class="nav-item">
                        <a asp-controller="News" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title"> Məlumatlar</span>
                        </a>
                    </li>

                   
                    
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Slide" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Ana Səhifə Slaydı</span>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a asp-controller="Contact" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-account menu-icon"></i>
                            <span class="menu-title">Elaqe sehifesi</span>

                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="LayoutService" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-account menu-icon"></i>
                            <span class="menu-title">INformasiya</span>

                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Message" asp-action="Index" class="nav-link">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Alinan MEsajlar</span>
                        </a>
                    </li>

                </ul>
            </nav>
            @RenderBody()
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    <!-- page-body-wrapper ends -->
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Admin/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/Admin/vendors/chart.js/Chart.min.js"></script>
    <script src="~/Admin/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/Admin/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/Admin/js/off-canvas.js"></script>
    <script src="~/Admin/js/hoverable-collapse.js"></script>
    <script src="~/Admin/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Admin/js/dashboard.js"></script>
    <script src="~/Admin/js/data-table.js"></script>
    <script src="~/Admin/js/jquery.dataTables.js"></script>
    <script src="~/Admin/js/dataTables.bootstrap4.js"></script>
    <!-- End custom js for this page-->
</body>

</html>

